  "bugs": {
    "url": "https://github.com/bestZwei/LibreDomains-beta/issues"
  },
  "homepage": "https://github.com/bestZwei/LibreDomains-beta#readme",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "jest": "^29.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
