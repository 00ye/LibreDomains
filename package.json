{
  "name": "libredomains-beta",
  "version": "1.0.0",
  "description": "Free subdomain distribution service powered by GitHub and Cloudflare",
  "main": "index.js",
  "scripts": {
    "validate": "node scripts/validate-request.js",
    "deploy": "node scripts/deploy-dns.js",
    "check-user": "node scripts/check-github-user.js",
    "health-check": "node scripts/health-check.js",
    "troubleshoot": "node scripts/troubleshoot.js",
    "fix-record": "node scripts/troubleshoot.js fix",
    "stats": "node scripts/generate-stats.js",
    "validate-token": "node scripts/validate-cloudflare-token.js",
    "test": "npm run validate requests/example-personal-website.json && npm run check-user example-user"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bestZwei/LibreDomains-beta.git"
  },
  "keywords": [
    "dns",
