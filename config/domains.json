{
  "domains": [
    {
      "name": "ciao.su",
      "enabled": true,
      "description": "基本二级域名",
      "cloudflare_zone_id": "your_cloudflare_zone_id_for_ciao_su"
    },
    {
      "name": "ciallo.de",
      "enabled": false,
      "description": "备用二级域名（暂未开放申请）",
      "cloudflare_zone_id": "your_cloudflare_zone_id_for_ciallo_de"
    }
  ],
  "record_types": ["A", "AAAA", "CNAME", "TXT", "MX"],
  "max_records_per_subdomain": 10,
  "cloudflare_timeout": 30,
